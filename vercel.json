{
  "version": 2,
  "headers": [
    { "source": "/(.*)", "headers": [
      { "key": "Cache-Control", "value": "no-store, no-cache, must-revalidate, max-age=0" },
      { "key": "Pragma", "value": "no-cache" },
      { "key": "Expires", "value": "0" }
    ]}
  ],
  "builds": [
    { "src": "api/proxy.js", "use": "@vercel/node" },
    { "src": "api/index.py", "use": "@vercel/python" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "css/**", "use": "@vercel/static" },
    { "src": "js/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/proxy", "dest": "/api/proxy.js" },
    { "src": "/api/user/(.*)", "dest": "/api/index.py" },
    { "src": "/api/deposit", "dest": "/api/index.py" },
    { "src": "/api/trade", "dest": "/api/index.py" },
    { "src": "/api/users", "dest": "/api/index.py" },
    { "src": "/api/pool/(.*)", "dest": "/api/index.py" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/js/(.*)", "dest": "/js/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}

